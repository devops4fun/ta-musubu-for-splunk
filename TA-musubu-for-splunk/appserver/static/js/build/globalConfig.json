        "configuration": {
            "tabs": [
                {
                    "options": {
                        "saveValidator": "function(formData) { if(!formData.proxy_enabled || formData.proxy_enabled === '0') {return true; } if(!formData.proxy_url) { return 'Proxy Host can not be empty'; } if(!formData.proxy_port) { return 'Proxy Port can not be empty'; } return true; }"
                    }, 
                    "name": "proxy", 
                    "entity": [
                        {
                            "field": "proxy_enabled", 
                            "type": "checkbox", 
                            "label": "Enable"
                        }, 
                        {
                            "field": "proxy_type", 
                            "defaultValue": "http", 
                            "options": {
                                "autoCompleteFields": [
                                    {
                                        "label": "http", 
                                        "value": "http"
                                    }, 
                                    {
                                        "label": "socks4", 
                                        "value": "socks4"
                                    }, 
                                    {
                                        "label": "socks5", 
                                        "value": "socks5"
                                    }
                                ], 
                                "disableSearch": true
                            }, 
                            "type": "singleSelect", 
                            "label": "Proxy Type"
                        }, 
                        {
                            "field": "proxy_url", 
                            "validators": [
                                {
                                    "minLength": 0, 
                                    "errorMsg": "Max host length is 4096", 
                                    "type": "string", 
                                    "maxLength": 4096
                                }
                            ], 
                            "type": "text", 
                            "label": "Host"
                        }, 
                        {
                            "field": "proxy_port", 
                            "validators": [
                                {
                                    "range": [
                                        1, 
                                        65535
                                    ], 
                                    "type": "number"
                                }
                            ], 
                            "type": "text", 
                            "label": "Port"
                        }, 
                        {
                            "field": "proxy_username", 
                            "validators": [
                                {
                                    "minLength": 0, 
                                    "errorMsg": "Max length of username is 50", 
                                    "type": "string", 
                                    "maxLength": 50
                                }
                            ], 
                            "type": "text", 
                            "label": "Username"
                        }, 
                        {
                            "field": "proxy_password", 
                            "encrypted": true, 
                            "validators": [
                                {
                                    "minLength": 0, 
                                    "errorMsg": "Max length of password is 8192", 
                                    "type": "string", 
                                    "maxLength": 8192
                                }
                            ], 
                            "type": "text", 
                            "label": "Password"
                        }, 
                        {
                            "field": "proxy_rdns", 
                            "type": "checkbox", 
                            "label": "Remote DNS resolution"
                        }
                    ], 
                    "title": "Proxy"
                }, 
                {
                    "name": "logging", 
                    "entity": [
                        {
                            "field": "loglevel", 
                            "defaultValue": "INFO", 
                            "options": {
