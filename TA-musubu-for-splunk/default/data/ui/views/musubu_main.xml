  <fieldset submitButton="true">
    <input type="text" token="musubu_ip_lookup">
      <label></label>
    </input>
  </fieldset>
  <row>
    <panel>
      <html>
        <style>
          #musubu_textinput_ip
          {
          width:250px
          }

        </style>
        <div id="musubu_threat_data_heading"/>
        <div id="musubu_textinput_ip"/>
        <div id="musubu_api_data"/>
      </html>
    </panel>
  </row>
  <row>
    <panel>
      <table id="musubu_tbl">
        <title>ip_test_data</title>
        <search>
          <query>| inputlookup ip_test_data</query>
          <!--index="musubu" sourcetype="csv" | table _time ip first last age state street zip -->
          <earliest>-24h@h</earliest>
          <latest>now</latest>
        </search>
        <option name="count">10</option>
        <option name="drilldown">none</option>
        <option name="refresh.display">progressbar</option>
      </table>
    </panel>
  </row>
